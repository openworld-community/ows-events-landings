{
   "$schema": "https://json.schemastore.org/prettierrc",
   "semi": true,
   "tabWidth": 3,
   "singleQuote": true,
   "printWidth": 100,
   "trailingComma": "es5",
   "singleAttributePerLine": true,
   "bracketSpacing": true,
   "bracketSameLine": false,
   "arrowParens": "avoid",
   "endOfLine": "lf",
   "plugins": ["prettier-plugin-tailwindcss"]
}
